#include "common.h"

struct 	v2p
{
	float2	tc0	: TEXCOORD0;	// base
};

//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
float4 main( v2p I ) : SV_Target
{
	float4 color = s_base.Sample( smp_rtlinear, I.tc0 );
	return float4(color.rgb, 0.f)*16.0f;
}
